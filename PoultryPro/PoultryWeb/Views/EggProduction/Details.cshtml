@model PoultryWeb.Models.EggProductionModel
@{
    ViewBag.Title = "Egg Production Details";
}

<h2>Egg Production Details</h2>

<dl class="dl-horizontal">
    <dt>Production ID</dt>
    <dd>@Model.ProductionId</dd>

    <dt>Flock ID</dt>
    <dd>@Model.FlockId</dd>

    <dt>Production Date</dt>
    <dd>@Model.ProductionDate.ToShortDateString()</dd>

    <dt>Egg Count</dt>
    <dd>@Model.EggCount</dd>

    <dt>Broken Eggs</dt>
    <dd>@Model.BrokenEggs</dd>

    <dt>Notes</dt>
    <dd>@Model.Notes</dd>
</dl>

<p>
    @Html.ActionLink("Back to List", "Index")
</p>
